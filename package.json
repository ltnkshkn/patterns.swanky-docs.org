{
  "name": "swanky-docs-patterns",
  "description": "Swanky Docs Patterns Demo",
  "scripts": {
    "start": "NODE_ENV=development node src/config/start/serve.dev.js",
    "prebuild": "rimraf docs/",
    "build": "npm run build:prod",
    "build:serve": "npm-run-all build serve:prod",
    "build:prod": "NODE_ENV=production webpack -p --progress --config src/config/build/build.prod.js --profile --colors",
    "serve:prod": "http-server docs -o"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "prismjs": "1.6.0"
  },
  "devDependencies": {
    "http-server": "0.9.0",
    "npm-run-all": "3.1.2",
    "rimraf": "2.5.4",
    "swanky": "2.0.2",
    "swanky-processor-ngdocs": "2.2.3",
    "webpack": "^2.2.0-rc"
  },
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:swanky-docs/demo.git",
    "type": "git"
  },
  "author": "Rod Leviton <rod@rodlevtion.com>",
  "license": "MIT"
}
